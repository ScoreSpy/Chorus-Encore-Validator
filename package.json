{
  "scripts": {
    "build": "npx tsc",
    "clean": "gts clean",
    "compile": "npm run build && pkg ./build/src/index.js --config package.json",
    "dev": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "fix": "gts fix",
    "lint": "gts lint",
    "posttest": "npm.cmd run lint",
    "prepare": "npm.cmd run tsc",
    "pretest": "npm.cmd run tsc",
    "tsc": "tsc"
  },
  "pkg": {
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.21",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "iconv-lite": "^0.6.3",
    "jszip": "^3.10.1",
    "midifile": "^2.0.0"
  }
}
